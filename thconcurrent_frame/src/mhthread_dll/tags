!_TAG_FILE_FORMAT	2	/extended format; --format=1 will not append ;" to lines/
!_TAG_FILE_SORTED	1	/0=unsorted, 1=sorted, 2=foldcase/
!_TAG_PROGRAM_AUTHOR	Darren Hiebert	/dhiebert@users.sourceforge.net/
!_TAG_PROGRAM_NAME	Exuberant Ctags	//
!_TAG_PROGRAM_URL	http://ctags.sourceforge.net	/official site/
!_TAG_PROGRAM_VERSION	5.8	//
LIBS	makefile	/^LIBS=libmhthread$/;"	m
LIBSPATH	makefile	/^LIBSPATH=..\/..\/lib$/;"	m
OBJS	makefile	/^OBJS=$(patsubst %.c,%.o,$(SRCS))$/;"	m
SRCS	makefile	/^SRCS=$(wildcard *.c)$/;"	m
TARGET	makefile	/^TARGET=$(LIBS).so $(LIBS).a$/;"	m
__THREAD_POOL_H__	include/thread_pool.h	2;"	d
_thread_pool	include/thread_pool.h	/^typedef struct _thread_pool$/;"	s
_thread_worker	include/thread_pool.h	/^typedef struct _thread_worker$/;"	s
arg	include/thread_pool.h	/^	void* 	arg;$/;"	m	struct:_thread_worker
curr_queue_size	include/thread_pool.h	/^	int		curr_queue_size;$/;"	m	struct:_thread_pool
function	include/thread_pool.h	/^	void* 	(*function) (void* arg);$/;"	m	struct:_thread_worker
is_shutdown	include/thread_pool.h	/^	int 		is_shutdown;$/;"	m	struct:_thread_pool
max_thread_num	include/thread_pool.h	/^	int		max_thread_num;$/;"	m	struct:_thread_pool
next	include/thread_pool.h	/^	struct 	_thread_worker *next;$/;"	m	struct:_thread_worker	typeref:struct:_thread_worker::_thread_worker
queue_head	include/thread_pool.h	/^	thread_worker	*queue_head;$/;"	m	struct:_thread_pool
queue_lock	include/thread_pool.h	/^	pthread_mutex_t	queue_lock;$/;"	m	struct:_thread_pool
queue_ready	include/thread_pool.h	/^	pthread_cond_t	queue_ready;$/;"	m	struct:_thread_pool
t_pool	thread_pool.c	/^static thread_pool* t_pool = NULL;$/;"	v	file:
thpool_add_worker	thread_pool.c	/^int thpool_add_worker(void* (*function) (void* arg), void* arg)$/;"	f
thpool_destroy	thread_pool.c	/^int thpool_destroy()$/;"	f
thpool_init	thread_pool.c	/^void thpool_init(int max_thread_num)$/;"	f
thread_pool	include/thread_pool.h	/^} thread_pool;$/;"	t	typeref:struct:_thread_pool
thread_routine	thread_pool.c	/^void* thread_routine(void* arg)$/;"	f
thread_worker	include/thread_pool.h	/^} thread_worker;$/;"	t	typeref:struct:_thread_worker
threadp	include/thread_pool.h	/^	pthread_t*	threadp;$/;"	m	struct:_thread_pool
